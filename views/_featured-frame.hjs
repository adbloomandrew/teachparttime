					airtable_row_id = "/" + session.id;
					action_type = "patch"
				}

				//
				//	->	Execute the query
				//
				$.ajax({
					url: 'https://api.airtable.com/v0/appMm7V8XWifHqCja/QKids' + airtable_row_id,
					type: action_type,
					dataType: 'json',
					headers: {
